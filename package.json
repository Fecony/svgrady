{
  "name": "svgrady",
  "version": "1.0.0",
  "description": "Javascript Separated Radial SVG generator",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "browser": "dist/bundle.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "dev": "rollup -cw",
    "deploy": "gh-pages -d example",
    "test": "jest --clearCache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint 'src/**/*.ts' 'tests/**/*.test.ts'",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "npm run lint",
      "prettier --write",
      "npm run test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fecony/svgrady.git"
  },
  "keywords": [
    "svg",
    "radial",
    "separated",
    "steps"
  ],
  "author": "Richard Tagil <tagils@mail.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fecony/svgrady/issues"
  },
  "homepage": "https://fecony.github.io/svgrady/",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.3",
    "babel-jest": "^25.1.0",
    "codecov": "^3.6.5",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslib": "^1.11.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.8.2"
  }
}
